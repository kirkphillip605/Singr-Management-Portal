# Database
DATABASE_URL="postgresql://postgres:[PASSWORD]@[HOST]:5432/[DB]"

# NextAuth.js
NEXTAUTH_URL="https://example.com"
NEXTAUTH_SECRET=""

# Google OAuth (optional - leave empty to disable Google sign-in)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Stripe
STRIPE_PUBLISHABLE_KEY=""
STRIPE_SECRET_KEY=""

# CRITICAL: This must be the webhook signing secret from Stripe dashboard
STRIPE_WEBHOOK_SECRET=""

STRIPE_API_VERSION="2025-05-28"

# HERE API (Required for venue search and geocoding)
HERE_API_KEY=""

# Sentry Config
SENTRY_AUTH_TOKEN=

# App Configuration
NODE_ENV="development"
LOG_LEVEL="debug"

# Prisma
PRISMA_LOG_LEVEL=query

# API Configuration
API_RATE_LIMIT_MAX="1000"
API_RATE_LIMIT_WINDOW="3600000"
